{
  "name": "@unmini/loader-ts",
  "type": "module",
  "version": "1.0.0-beta.2",
  "description": "Resolve ts file for unmini",
  "author": "Byron <byronogis@outlook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/byronogis",
  "homepage": "https://github.com/byronogis/unmini#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/byronogis/unmini.git"
  },
  "bugs": "https://github.com/byronogis/unmini/issues",
  "keywords": [
    "miniprogram",
    "miniapp",
    "conversion",
    "generation",
    "engine",
    "loader",
    "ts"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "start": "jiti src/index.ts",
    "automd": "automd README.md",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm publish",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "clear": "rimraf dist"
  },
  "dependencies": {
    "@ast-grep/napi": "^0.33.1",
    "@babel/core": "^7.26.7",
    "@babel/preset-typescript": "^7.26.0",
    "@types/babel__core": "^7.20.5",
    "@unmini/core": "workspace:*",
    "defu": "^6.1.4",
    "pathe": "^2.0.2",
    "type-fest": "^4.33.0"
  }
}
