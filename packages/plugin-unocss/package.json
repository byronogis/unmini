{
  "name": "@unmini/plugin-unocss",
  "type": "module",
  "version": "1.0.0-beta.1",
  "description": "Using unocss for unmini",
  "author": "Byron <byronogis@outlook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/byronogis",
  "homepage": "https://github.com/byronogis/unmini#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/byronogis/unmini.git"
  },
  "bugs": "https://github.com/byronogis/unmini/issues",
  "keywords": [],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "start": "jiti src/index.ts",
    "automd": "automd README.md",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm publish",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "clear": "rimraf dist"
  },
  "peerDependencies": {
    "@unocss/cli": "^65.4.3"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.4",
    "@unmini/core": "workspace:*",
    "fs-extra": "^11.3.0",
    "pathe": "^2.0.2"
  },
  "devDependencies": {
    "@unocss/cli": "^65.4.3"
  }
}
